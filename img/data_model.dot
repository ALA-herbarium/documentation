    //       //
    //       <TR><TD COLSPAN="2"><B>...</B></TD></TR>
    //       //
    //       <TR><TD ALIGN="LEFT"><I>p. key</I></TD>
    //       <TD ALIGN="LEFT">... . ..s</TD></TR>
    //       //
    //       <TR><TD ALIGN="LEFT"><I>p. key</I></TD>
    //       <TD ALIGN="LEFT">...</TD></TR>
    //       //
    //       <TR><TD ALIGN="LEFT"><I>default</I></TD>
    //       <TD ALIGN="LEFT" HREF="https://arctos.database.museum/info/ctDocumentation.cfm?table=ct...">... (ct...)</TD></TR>
    //       </TABLE>>
    //       ]
